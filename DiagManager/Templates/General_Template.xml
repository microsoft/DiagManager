      </Features>
      <Versions>
        <Version name="10.50" enabled="true"/>
        <Version name="11" enabled="true"/>
        <Version name="12" enabled="true"/>
        <Version name="13" enabled="true"/>
        <Version name="14" enabled="true"/>
        <Version name="15" enabled="true"/>      
