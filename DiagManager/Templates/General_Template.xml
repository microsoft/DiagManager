        <Feature name="SQL" enabled="true"/>

      </Features>
      <Versions>
        
        <Version name="13" enabled="true"/>
        <Version name="14" enabled="true"/>
        <Version name="15" enabled="true"/>      
      </Versions>
    </Scenario>

    <Scenario name="PolyBase" friendlyname="PolyBase" target="file"  DefaultChecked="false">
      <Features>
        <Feature name="SQL" enabled="true"/>

